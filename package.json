{
  "name": "@fooloomanzoo/input-picker-pattern",
  "version": "2.3.8",
  "authors": [
    "Johannes Brautzsch <fooloomanzoo>"
  ],
  "description": "A pattern for polymer elements to create a picker for an input",
  "homepage": "https://github.com/fooloomanzoo/input-picker-pattern/",
  "keywords": [
    "webcomponents",
    "polymer",
    "picker",
    "pattern",
    "polyfill",
    "input"
  ],
  "main": "input-picker-pattern.html",
  "scripts": {
    "postinstall": "bower install",
    "prepublish": "npm run build && git add . && git commit -m \"publish new version\" && npm version patch && git push",
    "build": "git pull && bower update && npm run analyze",
    "start": "npm run analyze && polymer serve -o",
    "analyze": "polymer analyze > analysis.json",
    "test": "polymer test",
    "test:p": "polymer test -p --config-file=wct.p.conf.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fooloomanzoo/datetime-picker.git"
  },
  "license": "MIT",
  "dependencies": {
    "@polymer/polymer": "^2.6.1",
    "@polymer/iron-overlay-behavior": "^2"
  },
  "devDependencies": {
    "web-component-tester": "^6.5.0",
    "@webcomponents/webcomponentsjs": "^1",
    "@polymer/iron-component-page": "^3",
    "@polymer/iron-flex-layout": "^2",
    "@polymer/iron-demo-helpers": "^2",
    "@polymer/iron-form": "^2"
  }
}
